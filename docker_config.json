{
    "name": "BdsCore",
    "docker_image": "bdsmaneger/maneger:latestdebug",
    "dockerfile": "Dockerfile",
    "ports": [
        {
            "port": 19132,
            "external": 19132,
            "protocoll": "udp"
        },
        {
            "port": 19133,
            "external": 19133,
            "protocoll": "udp"
        },
        {
            "port": 80,
            "external": 8880,
            "protocoll": "tcp"
        }
    ],
    "mounts": [
        {
            "path": "/home/bds/bds_core/",
            "from": "${HOME}/bds_core/"
        }
    ],
    "env": [
        {
            "name": "TELEGRAM_TOKEN",
            "value": null
        },
        {
            "name": "DESCRIPTION",
            "value": "Docker Test"
        },
        {
            "name": "WORLD_NAME",
            "value": "Docker Run Test"
        },
        {
            "name": "GAMEMODE",
            "value": "survival"
        },
        {
            "name": "DIFFICULTY",
            "value": "normal"
        },
        {
            "name": "XBOX_ACCOUNT",
            "value": false
        },
        {
            "name": "PLAYERS",
            "value": 8
        },
        {
            "name": "BDS_VERSION",
            "value": "latest"
        },
        {
            "name": "SERVER",
            "value": "bedrock"
        }
    ],
    "target": "bdscore",
    "options": [
        "--net=host"
    ]
}